{
    "python.testing.pytestArgs": [
        "tests"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    
    // Tell cSpell which language-ids (file types) to check.
    // Anything set to false is skipped.
    "cSpell.enabledFileTypes": {
      "md": true,   
      "py": true,
      "ipynb": true,
      "csv": false
    },
    "cSpell.words": [
      "AOUSC",
      "autoreload",
      "crosswalked",
      "crosswalking",
      "figsize",
      "ipython",
      "Juris",
      "latestaction",
      "middlename",
      "nameparser",
      "nids",
      "pathlib",
      "rapidfuzz",
      "receiveddate",
      "urlparse"
    ],
}